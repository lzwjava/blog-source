{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "bash fix_template.sh && jekyll build",
                "outputDirectory": "_site"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/_site/$1"
        }
    ]
}